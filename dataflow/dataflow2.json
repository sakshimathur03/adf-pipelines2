{
	"name": "dataflow2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "filter1"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          id as string,",
				"          title as string,",
				"          Rating as string,",
				"          maincateg as string,",
				"          platform as string,",
				"          actprice1 as string,",
				"          norating1 as string,",
				"          noreviews1 as string,",
				"          star_5f as string,",
				"          star_4f as string,",
				"          star_3f as string,",
				"          star_2f as string,",
				"          star_1f as string,",
				"          fulfilled1 as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 filter(toInteger(norating1)<10 || toInteger(noreviews1)<10) ~> filter1",
				"filter1 derive() ~> derivedColumn1"
			]
		}
	}
}