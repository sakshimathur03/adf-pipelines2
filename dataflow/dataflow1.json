{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ProductsTable",
						"type": "DatasetReference"
					},
					"name": "productSchema"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "Markup"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ProductID as integer,",
				"          ProductName as string,",
				"          Price as decimal(10,2),",
				"          Quantity as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> productSchema",
				"productSchema derive(Markup = Price/Quantity) ~> Markup",
				"Markup derive() ~> derivedColumn1"
			]
		}
	}
}